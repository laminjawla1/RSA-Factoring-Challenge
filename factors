#!/usr/bin/python3
from sys import argv, exit


def main():
    if len(argv) != 2:
        print("factors <file>")
        exit(-1)
    with open(argv[1], "r") as f:
        line = f.readline()
        while line:
            number = int(line.strip())
            print_factors(number)
            line = f.readline()


def print_factors(number):
    i = 2
    for i in range(2, number):
        if not number % i:
            print(f"{number}={number // i}*{i}")
            break


if __name__ == "__main__":
    main()
